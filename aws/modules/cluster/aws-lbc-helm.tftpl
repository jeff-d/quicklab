export KUBECONFIG=$KUBECONFIG:~/.kube/${CLUSTERNAME}
helm version # taint if helm not present
helm repo add eks https://aws.github.io/eks-charts
helm upgrade -i aws-load-balancer-controller eks/aws-load-balancer-controller -n kube-system --atomic --set clusterName=${CLUSTERNAME} --set serviceAccount.create=false --set serviceAccount.name=${SERVICEACCOUNTNAME} --set defaultTags.Module=${MODULE} --set defaultTags.LabId=${LABID} --set defaultTags.CreatedBy=${CREATEDBY}